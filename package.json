  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ssh2-sftp-client": "^9.0.0",
    "aws-cdk": "^2.0.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.0.0",
    "vitest": "^0.34.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
